    "version": "0.2.0",
    "configurations": [
        {
            "name": "NugetMcpServer Http Debug",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/NugetMcpServer/bin/Debug/net9.0/win-x64/NugetMcpServer.exe",
            "args": ["--http", "--port", "8080"],
            "cwd": "${workspaceFolder}/NugetMcpServer/",
            "console": "internalConsole",
            "stopAtEntry": false,
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            }
        }


    ]
}
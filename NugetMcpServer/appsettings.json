{
  "NuGet": {
    "Sources": [
      {
        "Name": "nuget.org",
        "Url": "https://api.nuget.org/v3-flatcontainer/",
        "IsEnabled": true,
        "Priority": 100
      },
      {
        "Name": "azure-devops-example",
        "Url": "https://pkgs.dev.azure.com/your-org/_packaging/your-feed/nuget/v3/index.json",
        "Username": "your-email@company.com",
        "Password": "your-personal-access-token",
        "IsEnabled": false,
        "Priority": 80,
        "IsAzureDevOps": true,
        "Organization": "your-org",
        "FeedId": "your-feed",
        "FilterNativePackagesOnly": true
      }
    ],
    "DefaultTimeoutSeconds": 30,
    "MaxRetryAttempts": 3
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  }
}
